package Entidades;


public class Animais extends Taxonomia{
	
public Animais(String reino, String filo, String classe, String ordem, String familia, String genero, String especie){
			super(reino,filo,classe,ordem,familia,genero,especie);
	}


	public getInfo() {
		
		System.out.println(reino+filo+classe+ordem+familia+);
		
	}



}